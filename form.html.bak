                    <ul class="collection">
                        <li class="collection-item">
                            <div class="row tableFilter">
                                <form>
                                    <div class="input-field col s2">
                                        <select>
                                            <option value="" 下拉列表>下拉列表</option>
                                            <option value="1">选项 1</option>
                                            <option value="2">选项 2</option>
                                            <option value="3">选项 3</option>
                                        </select>
                                        <label></label>
                                    </div>
                                    <div class="input-field col s3">
                                        <input id="last_name" type="text" class="validate">
                                        <label for="last_name">姓名</label>
                                    </div>
                                    <div class="input-field col s3">
                                        <input id="input2" type="text" class="validate">
                                        <label for="input2">职位</label>
                                    </div>
                                    <div class="input-field col s3">
                                        <input id="input3" type="text" class="validate">
                                        <label for="input3">警号</label>
                                    </div>
                                    <div class="input-field col s1">
                                        <button class="btn waves-effect waves-light z-depth-0" type="submit" name="action">搜索
                                            <!--<i class="material-icons right">search</i>-->
                                        </button>
                                    </div>
                                </form>
                            </div>
